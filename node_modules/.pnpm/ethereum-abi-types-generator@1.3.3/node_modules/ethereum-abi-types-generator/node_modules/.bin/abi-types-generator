#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/dist/bin/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/dist/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/dist/bin/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/dist/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules/ethereum-abi-types-generator/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/ethereum-abi-types-generator@1.3.3/node_modules:/Users/dzik/Projects/golem/mmpg/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/generator-cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/generator-cli.js" "$@"
fi
